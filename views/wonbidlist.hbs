<title>Won Bid Items</title>
<div class="limiter">
    <div class="container-main-content container-main-content-top">
        <div class="wrap-main-content wrap-item-view wrap-main-content-top">
            <table class="table table-hover table-responsive">
                <h1>Won Bidding List</h1>
                <br>
                <br>
                <br>
                <br>
                <thead>
                    <tr>
                        <th>
                            <h3>Product Bid ID</h3>
                        </th>
                        <th>
                            <h3>Product Model</h3>
                        </th>
                        <th>
                            <h3>Review</h3>
                        </th>
                    </tr>
                </thead>
                <tbody>
                    {{#each items}}
                    <tr>
                        <td scope="row">
                            <a type="button" href="/item/{{ProductID}}">
                                <h3>{{ProductID}}</h3>
                            </a>
                        </td>

                        <td scope="row">
                            <a type="button" href="/item/{{ProductID}}">
                                <h3>{{Model}}</h3>
                            </a>
                        </td>
                        <td scope="row">
                            <form action="/FinishBidItemList/{{seller_id}}/{{ProductID}}/0" method="POST" id="frmsubmit">
                            </form>
                            <a name="" id="" class="btn btn-primary" href="javascript: $('#frmsubmit').submit()"
                                role="button">
                                <i class="fa fa-comment" aria-hidden="true"></i>
                            </a>
                        </td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>